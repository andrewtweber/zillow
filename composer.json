{
    "name": "dfreer/zillow",
    "type": "application",
    "description": "Zillow PHP API Wrapper",
    "keywords": ["zillow", "php", "api"],
    "homepage": "https://github.com/vinceg/zillow",
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Gabriel",
            "email": "vadimg88@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "guzzlehttp/guzzle": "5.3|~6.0",
        "fabpot/goutte": "^3.1"
    },
    "require-dev": {
